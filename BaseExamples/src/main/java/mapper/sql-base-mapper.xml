<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sendtomoon.mybatis.baseExamples.BaseDAO">
	<insert id="add" parameterType="table1">
		insert into
			TABLE1
		(id,name,price,created_date)
		values
			(#{id},#{name},#{price},sysdate)
	</insert>

</mapper>